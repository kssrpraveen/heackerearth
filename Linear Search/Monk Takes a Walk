
// Write your code here
#include <stdio.h>
#include <string.h>
int main()
{
    int t;
    scanf("%d",&t);
    for(int i=0;i<t;i++)
    {
        char *a=(char*)malloc(sizeof(char)*100000);
        scanf("%s",a);
        int c=0;
        for(int j=0;j<strlen(a);j++)
        {
            
            if(a[j]=='a'||a[j]=='e'||a[j]=='i'||a[j]=='o'||a[j]=='u'||a[j]=='A'||a[j]=='E'||a[j]=='I'||a[j]=='O'||a[j]=='U')
            {
                c++;
            }
        }
        printf("%d\n",c);
    }
    return 0;
}
