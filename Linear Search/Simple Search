#include<stdio.h>
int main()
{
    int n,k;
    scanf("%d",&n);
    int *a=(int*)malloc(sizeof(int)*n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        
    }
    scanf("%d",&k);
    for(int i=0;i<n;i++)
    {
        if(a[i]==k)
        {
            printf("%d",i);
            return 0;
        }
    }
    return 0;
}
